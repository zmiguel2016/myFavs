<div class="song-form">
  <h2 class="page-header">Search Songs</h2>
  <a class="btn btn-primary" href="/songs/new">Manually add song</a>
</div>

<form action="/spotifysearch/newSpotify" method="GET">
  <div class="form-row">
    <div class="form-item">
      <label>Search song</label>
      <input type="text" name="title" value="<%= searchOptions.title %>" />
    </div>
  </div>
  <div class="form-row form-row-end">
    <button class="btn btn-primary" type="submit">Search</button>
  </div>
</form>
<br />
<%- include('../partials/spotifyGrid', { songs: songs}) %>
